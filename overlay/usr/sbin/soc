#!/bin/sh

cpuid=$((($(devmem 0x1300002C) >> 12) & 0xFFFF))
if [ $cpuid -eq $((0x40)) ] || [ $cpuid -eq $((0x41)) ]; then
	reg=0x13540250
else
	reg=0x13540238
fi

if [ "$1" == "-f" ]; then
	[ "$cpuid" -eq $((0x2000)) ] && cpuid=0x20
	printf 't%X\n' $cpuid
	exit
fi

cpu_sig=$(printf '0x%08X' $(((cpuid << 16) | $((($(devmem $reg) >> 16) & 0xFFFF)))))
case "$cpu_sig" in
	0x00050000) soc="t10" ;;
	0x20001111) soc="t20n" ;;
	0x20002222) soc="t20x" ;;
	0x20003333) soc="t20l" ;;
	0x00211111) soc="t21n" ;;
	0x00213333) soc="t21l" ;;
	0x00215555) soc="t21z" ;;
	0x00231111) soc="t23n" ;;
	0x00301111) soc="t30n" ;;
	0x00302222) soc="t30x" ;;
	0x00303333) soc="t30al" ;;
	0x00304444) soc="t30a" ;;
	0x00305555) soc="t30z" ;;
	0x00311111) soc="t31n" ;;
	0x00312222) soc="t31x" ;;
	0x00313333) soc="t31l" ;;
	0x00314444) soc="t31a" ;;
	0x00315555) soc="t31z" ;;
	0x00316666) soc="t31zx" ;;
	0x0031DDDD) soc="t31zc" ;;
	0x0031EEEE) soc="t31lc" ;;
	0x00401111) soc="t40l" ;;
	0x00404444) soc="t40a" ;;
	0x00407777) soc="t40xp" ;;
	0x00408888) soc="t40nn" ;;
	0x00411111) soc="t41n" ;;
	0x00413333) soc="t41l" ;;
	0x00414444) soc="t41a" ;;
	0x00415555) soc="t41zl" ;;
	0x00416666) soc="t41zx" ;;
	0x00417777) soc="t41zn" ;;
	0x00418888) soc="t41lc" ;;
	0x00419999) soc="t41lq" ;;
	0x0041AAAA) soc="t41nq" ;;
	#0x0041????) soc="t41xq" ;;
	*)
		echo "Unknown SoC: $cpu_sig"
		exit 1
		;;
esac

echo $soc

exit 0
